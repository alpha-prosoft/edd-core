<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} %msg%n</pattern>
        </encoder>
    </appender>
    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>
    <logger level="TRACE" name="com.zaxxer.hikari"></logger>
    <logger level="DEBUG" name="logging.level.com.zaxxer.hikari.HikariConfig"></logger>

</configuration>
